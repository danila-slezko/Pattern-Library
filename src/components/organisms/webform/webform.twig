    <div class="margin-bottom-xs">
      <div class="grid gap-xxs items-center@md">
        <div class="col-4@md text-right@md">
          <label class="form-label" for="phone">Company Phone</label>
        </div>
        <div class="col-4@md">
          <input class="form-control width-100%" id="phone" type="text" name="phone">
        </div>
      </div>
    </div>
    <div class="margin-bottom-xs">
      <div class="grid gap-xxs items-center@md">
        <div class="col-4@md text-right@md">
          <label class="form-label" for="email">Work Email</label>
        </div>
        <div class="col-4@md">
          <input class="form-control width-100%" id="email" type="email" name="email">
        </div>
      </div>
    </div>
    <div class="margin-bottom-xs">
      <div class="grid gap-xxs items-center@md">
        <div class="col-4@md text-right@md">
          <label class="form-label" for="region">Region</label>
        </div>
        <div class="col-4@md">
          <div class="select">
            <select id="00N2p000009H8t7" name="00N2p000009H8t7" class="select__input form-control width-100%" id="country" name="country">

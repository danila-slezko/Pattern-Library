  <div class="text-center">
    <p class="color-contrast-medium">CNET Content Solutions will use your personal information to manage the partner program.</p>
    <br>
    {% include '@button' with { buttonText: 'Submit', buttonModifier: 'btn--primary' } %}
  </div>
  </form>
</section>
